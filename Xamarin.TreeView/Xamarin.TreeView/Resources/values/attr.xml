<?xml version="1.0" encoding="utf-8" ?>
<resources>

  <declare-styleable name="TreeView">

    <attr name="trace_visibility" format="reference|enum">
      <enum name="visible" value="0"/>
      <enum name="invisible" value="2"/>
    </attr>
    <attr name="trace_color" format="reference|color"/>
    <attr name="trace_width" format="reference|dimension"/>
    <attr name="trace_margin" format="reference|dimension"/>

    <attr name="node_margin" format="reference|dimension"/>
    <attr name="head_margin" format="reference|dimension"/>

  </declare-styleable>

</resources>